generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id @default(uuid())
  name          String
  email         String @unique
  hash_password String

  @@map("users")
}

model Gym {
  id String @id @default(uuid())
  title String
  description String?
  phone String?
  latitude Decimal
  longitude Decimal
}